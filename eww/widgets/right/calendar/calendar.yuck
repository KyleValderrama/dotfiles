(defwindow calendar
    :monitor 0
    :namespace "calendar"
    :class "calendar-widget"
    :geometry
        (geometry
            :x "15px"
            :y "5px"
            :width "200px"
            :anchor "top right"
            :hexpand true
        )
    :stacking "fg"
    :windowtype "dock"
    (box
        :class "outer-box-calendar"
        (box
            :class "inner-box-calendar"
            (calendar
                :class "calendar-component"
            )
        )
    )
)
